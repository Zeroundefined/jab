#! /bin/bash

# set -x

test -f $TRACKERS_SERVER/bin/add_to_a_path.sh && source $TRACKERS_SERVER/bin/add_to_a_path.sh
add_to_a_path PYTHONPATH $JAB/python/site
if [[ -f /usr/local/bin/ranger ]]
then ranger=/usr/local/bin/ranger
else ranger=$(which ranger 2> /dev/null)
fi
if [[ -z $ranger ]]
then echo Cannot find ranger
else
	echo -n "[?47h"
	/usr/local/bin/python2.7 $ranger $*
	echo -n "[?47l"
fi
