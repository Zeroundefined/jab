#! /bin/bash

first_dir()
{
	dir=$(pwd)
	for d in "$@"
	do
		if [[ -d $d ]]
		then
			dir=$(python -c"import os; print os.path.realpath(\"$d\")")
			return 0
		fi
	done
	return 1
}

last_dir()
{
	dir=$(pwd)
	for d in "$@"
	do
		if [[ -d $d ]]
		then
			dir=$(python -c"import os; print os.path.realpath(\"$d\")")
		fi
	done
}

