#! /bin/bash

. ~/.jab/bin/first_dir

echo $* | grep -q pycos && remove="rf pycos" || remove=rf
$remove $*
first_dir $*
echo $* | grep -q deep && options= || options="-maxdepth 1"
find $dir $options \( -type d -a -name .svn -prune \) -o -type d -exec $remove $* {} \;
if [[ $0 != "./clean" ]]
then
	test -f ./clean && sh ./clean
fi
